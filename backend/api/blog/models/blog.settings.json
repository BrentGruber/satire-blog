{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Published": {
      "type": "date"
    },
    "Body": {
      "type": "richtext"
    },
    "Slug": {
      "type": "string"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
